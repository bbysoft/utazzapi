  "paths": {
    "/manager/hotels": {
      "get": {
        "summary": "Hotels list",
        "tags": [],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    },
                    "result": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "hotelID": {
                            "type": "integer"
                          },
                          "name": {
                            "type": "string"
                          }
                        }
                      }
                    }
                  }
                },
                "examples": {
                  "example-1": {
                    "value": {
                      "message": "success",
                      "result": [
                        {
                          "hotelID": 1,
                          "name": "Hotel 1"
                        },
                        {
                          "hotelID": 2,
                          "name": "Hotel 2"
                        }
                      ]
                    }
                  }
                }
              }
            },
            "headers": {}
          }
        },
        "operationId": "get-manager-hotels",
        "description": "List all hotels",
        "security": [
          {
            "API Key": []
          },
          {
            "sandbox http auth": []
          }
        ]
      }
    },
    "/manager/hoteldata": {
      "post": {
        "summary": "Hotel data",
        "operationId": "post-manage-hoteldata",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "hotelID": {
                      "type": "integer"
                    },
                    "name": {
                      "type": "string"
                    },
                    "desc": {
                      "type": "string"
                    },
                    "city": {
                      "type": "string"
                    },
                    "images": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "integer"
                          },
                          "desc": {
                            "type": "string"
                          },
                          "url": {
                            "type": "string"
                          }
                        }
                      }
                    }
                  }
                },
                "examples": {
                  "example-1": {
                    "value": {
                      "hotelID": 1,
                      "name": "test hotel 1",
                      "desc": "description of test hotel 1",
                      "city": "hotelcity",
                      "images": [
                        {
                          "id": 1,
                          "desc": "room image",
                          "url": "http://domain.com/images/hotel.img"
                        }
                      ]
                    }
                  }
                }
              }
            }
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "hotelID": {
                    "type": "integer"
                  }
                }
              },
              "examples": {
                "example-1": {
                  "value": {
                    "hotelID": 1
                  }
                }
              }
            }
          }
        },
        "parameters": []
      },
      "parameters": []
    }
  },
  "components": {
    "schemas": {},
    "securitySchemes": {
      "API Key": {
        "name": "api_token",
        "type": "apiKey",
        "in": "query"
      },
      "sandbox http auth": {
        "type": "http",
        "scheme": "basic"
      }
    }
  },
